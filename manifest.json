{
	"manifest_version": 2,
	"name": "Send to aria2",
	"description": "Send download links to aria2. Filtering most common links (EliteTorrent, txibit, divxtotal, ...)",
	"version": "1.3",
	"permissions": [
		"contextMenus",
		"clipboardWrite",
		"clipboardRead",
		"notifications",
		"http://blit.us.to:6800/",
		"http://aria.tocant.me:6800/"
	],
	"icons": {
		"16": "icon-16.png",
		"48": "icon-48.png",
		"128": "icon-128.png",
		"240": "icon-240.png"
	},
	"web_accessible_resources": [
		"error.gif"
	],
	"background": {
		"scripts": ["ariaSend.js"]
	}
}
